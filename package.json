{
    "name": "susanoo-cms",
    "version": "0.0.11",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "build:package": "tsup index.tsx --format cjs,esm --dts",
        "lint:package": "tsc",
        "release": "npm run build:package && changeset publish",
        "db:introspect": "dotenv -- prisma introspect",
        "db:generate": "dotenv -- prisma generate",
        "postinstall": "npx prisma generate",
        "style:build": "npm-run-all --parallel copy:* build:*",
        "build:styles": "npm-run-all sass:build postcss:build",
        "build:images": "imagemin src/assets/pics/*.{jpg,png,svg} --out-dir=public",
        "copy:fonts": "copyfiles src/assets/fonts/**/* public --flat",
        "watch": "npm-run-all --parallel watch:*",
        "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
        "sass:build": "sass src/assets/styles:public --no-source-map",
        "sass:watch": "sass src/assets/styles:src/assets/build --no-source-map --watch",
        "postcss:build": "postcss public/*.css --replace --env production",
        "postcss:watch": "postcss src/assets/build/*.css --watch --dir public",
        "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json,js,mjs,cjs,scss}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,md,mdx,json,js,mjs,cjs,scss}\""
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@formkit/auto-animate": "^1.0.0-beta.3",
        "@mantine/hooks": "^6.0.17",
        "@next-auth/prisma-adapter": "^1.0.4",
        "@prisma/client": "^4.1.1",
        "@trpc/client": "^9.26.2",
        "@trpc/next": "^9.26.2",
        "@trpc/react": "^9.26.2",
        "@trpc/server": "^9.26.2",
        "bcryptjs": "^2.4.3",
        "chart.js": "^3.9.1",
        "formik": "^2.2.9",
        "framer-motion": "^7.6.1",
        "next": "12.2.3",
        "next-auth": "^4.10.3",
        "next-i18next": "^12.1.0",
        "next-seo": "^6.1.0",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-chartjs-2": "^4.3.1",
        "react-dom": "18.2.0",
        "react-intersection-observer": "^9.4.0",
        "react-query": "^3.39.2",
        "react-router-dom": "^6.10.0",
        "react-switch": "^7.0.0",
        "react-uuid": "^2.0.0",
        "superjson": "^1.9.1",
        "yup": "^0.32.11",
        "zod": "^3.17.3",
        "zustand": "^4.1.4"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "@swc/core": "^1.3.88",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^18.0.0",
        "@types/react": "18.0.14",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "18.0.5",
        "autoprefixer": "^10.4.8",
        "copyfiles": "^2.4.1",
        "cssnano": "^5.1.12",
        "dotenv-cli": "^7.3.0",
        "eslint": "8.18.0",
        "eslint-config-next": "12.2.3",
        "imagemin": "^8.0.1",
        "imagemin-cli": "^7.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.14",
        "postcss-cli": "^9.1.0",
        "postcss-csso": "^6.0.0",
        "prettier": "^2.7.1",
        "prisma": "^4.1.1",
        "sass": "^1.54.0",
        "ts-node": "^10.9.1",
        "tsup": "^7.2.0",
        "typescript": "^4.7.4"
    }
}
