{
  "name": "susanoo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "style:build": "npm-run-all --parallel copy:* build:*",
    "build:styles": "npm-run-all sass:build postcss:build",
    "build:images": "imagemin src/assets/pics/*.{jpg,png} --out-dir=public",
    "copy:fonts": "copyfiles src/assets/fonts/**/* public --flat",
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
    "sass:build": "sass src/assets/styles:public --no-source-map",
    "sass:watch": "sass src/assets/styles:src/assets/build --no-source-map --watch",
    "postcss:build": "postcss public/*.css --replace --env production",
    "postcss:watch": "postcss src/assets/build/*.css --watch --dir public"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.1.1",
    "@trpc/client": "^9.26.2",
    "@trpc/next": "^9.26.2",
    "@trpc/react": "^9.26.2",
    "@trpc/server": "^9.26.2",
    "next": "12.2.3",
    "next-auth": "^4.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "superjson": "^1.9.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.8",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.12",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.3",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-csso": "^6.0.0",
    "prisma": "^4.1.1",
    "sass": "^1.54.0",
    "typescript": "4.7.4"
  }
}
